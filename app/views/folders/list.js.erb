
$('<%= escape_javascript(render 'notes/new', :pubmed_id => @pubmed_id) %>').insertAfter('<%= '#folder_list_' + @pubmed_id.to_s %>');

$('<%= '#folder_list_' + @pubmed_id.to_s %>').next().toggle('300');
$('<%= '#folder_list_' + @pubmed_id.to_s %>').on('click', function(e){
	$(this).next().toggle('300');
	return false;
});
