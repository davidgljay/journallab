$('.feed_nav_item').removeClass('feed_nav_selected');
$('.homePageFeed').hide();
$('<%= '.' + @switchto %>').val('<%= @nav_language %>')
$('<%= '.' + @switchto %>').addClass('feed_nav_selecte');
$('<%= '#' + @switchto + '_feed'%>').html('<%= escape_javascript(render('/feed/' + @switchto_render)) %>'); 
$('<%= '#' + @switchto + '_feed'%>').show();
