<% if signed_in? %>
  <table class="front" summary="For signed-in users">
    <tr>
      <td class="main">
<%= form_tag('/lookup', :method => "get", :class => "homepage_search round") do  %>
<%= label_tag(:pubmed_id, "Go ahead, enter a Pubmed ID:") %><br>
  <%= text_field_tag(:pubmed_id) %><br>
  <%= submit_tag("Submit") %>
<% end %>
      </td>
      <td class="sidebar round">
        <%= render 'shared/user_info' %>
        <em>List of recently viewed papers will go here.</em> 
      </td>
    </tr>
  </table>
<% end %>
