 <header>
       <div id='header'>
            <div id='utNav'>
            <%= link_to "Home", root_path %>
            <% if signed_in? %>
              <% if current_user.admin? %>
              <%= link_to "Add Users", bulksignup_path %>
              <%= link_to "Reset Passwords", reset_password_path %>
              <%= link_to "Dashboard", dashboard_path %>
              <% end %>
            <%= link_to "Profile", current_user %>
            <%= link_to "Settings", edit_user_path(current_user) %>
	    <%= link_to "Sign out", signout_path, :method => :delete %>
            <% else %>                  
            <%= link_to "Sign in", signin_path %>
            <% end %>

            <% unless @title == 'Home' %>

                <div id = 'contentSearch'>
		<table border="0" align="right" cellpadding="0" cellspacing="0">
		      <tbody><tr>
        		<td>
                	<%= form_tag('/lookup', :method => "get") do  %>
                	<%= text_field_tag(:pubmed_id, "Search PubMed with style.", :onfocus =>  "if(this.value == 'Search PubMed with style.') { this.value = ''; }", :class => "inputSearch" ) %> </td>
                	<td><%= image_submit_tag("button_go.png") %></td>
                	<% end %>
			</td>
      			</tr>
    		       </tbody></table>

                </div>
         </div>
     <% logo = image_tag("journal_lab_logo2.png", :alt => "Journal Lab", :width => '160', :height => '90') %>
     <%= link_to logo, root_path %>
     </div>
<% end %>

