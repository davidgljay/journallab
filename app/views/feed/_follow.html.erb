<div id= <%= @follow.name.downcase + "Feed" %>>
	<% @feed.each do |paper| %>
	   <div class = "search_results">
	   <%= render 'papers/list', :paper => paper %>
	  </div>
	<% end %>
	<div style = "margin: 10px; margin-left: 25px;">
		<%= link_to "See more", "/lookup/" + @follow.name.downcase + "?page=3" %>
	</div>
</div>
