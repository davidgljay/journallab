$('.feed_nav_item').removeClass('feed_nav_selected');
$('.homePageFeed').hide();
$('<%= '.' + @switchto %>').find('input').val('<%= @nav_language %>')
$('<%= '.' + @switchto %>').addClass('feed_nav_selected');
$('<%= '.' + @switchto %>').parent().prev().addClass('hideonload');
$('<%= '#' + @switchto + '_feed'%>').html('<%= escape_javascript(render '/feed/' + @switchto_render, :group => @group) %>'); 
$('<%= '#' + @switchto + '_feed'%>').show();
$('.feed_remove').hide();
