<% @feed.each do |item| %>
   <div class = "feeditem" <%= "style = 'background:#BDD;'" if item[4]%>>
    <strong><%= item[0].firstname + ' ' + item[0].lastname.first + '.' %></strong> <%= item[1] %> <strong><%= link_to item[2].title, item[2] %></strong>
     <br>
     <em><%= item[3].strftime('%B %d, %Y') %></em>
    </div>
<% end %>
